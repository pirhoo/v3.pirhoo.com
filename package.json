{
  "name": "v3.pirhoo.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build",
    "deploy": "push-dir --dir=dist --branch=gh-pages",
    "predeploy": "yarn run build",
    "csv": "gulp csv"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "axios": "^0.18.0",
    "bootstrap": "^4.4.1",
    "chroma-js": "^2.1.0",
    "d3": "^5.15.0",
    "d3-tip": "^0.9.1",
    "granim": "^2.0.0",
    "lodash": "^4.17.15",
    "vue": "^2.6.11",
    "vue-lazyload": "^1.3.3",
    "vue-masonry-css": "^1.0.3",
    "vue-perfect-scrollbar": "^0.2.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "async": "^2.6.2",
    "csv-stringify": "^5.3.6",
    "gh-pages": "^2.2.0",
    "gulp": "^4.0.2",
    "gulp-convert": "^0.2.0",
    "gulp-filter": "^6.0.0",
    "gulp-image-resize": "^0.13.1",
    "gulp-json-editor": "^2.5.4",
    "gulp-load-plugins": "^2.0.2",
    "image-size": "^0.7.3",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.13.0",
    "node-vibrant": "^3.2.0-alpha",
    "path": "^0.12.7",
    "push-dir": "^0.4.1",
    "sass-loader": "^7.1.0",
    "sass-variable-loader": "^0.1.2",
    "simple-git": "^2.20.1",
    "slug": "^1.1.0",
    "through2": "^3.0.1",
    "vue-template-compiler": "^2.6.11",
    "webshot": "^0.18.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "import/no-webpack-loader-syntax": false,
      "max-len": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
