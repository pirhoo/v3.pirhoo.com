    "node-sass": "^4.9.0",
    "path": "^0.12.7",
    "push-dir": "^0.4.1",
    "sass-loader": "^7.0.1",
    "sass-variable-loader": "^0.1.2",
    "slug": "^0.9.1",
    "through2": "^2.0.3",
    "vue-template-compiler": "^2.5.16",
    "webshot": "^0.18.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "import/no-webpack-loader-syntax": false
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
