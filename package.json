{
  "name": "v3.pirhoo.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build",
    "deploy": "push-dir --dir=dist --branch=gh-pages",
    "csv": "gulp csv"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.1",
    "@fortawesome/free-brands-svg-icons": "^5.1",
    "@fortawesome/free-regular-svg-icons": "^5.1.1",
    "@fortawesome/free-solid-svg-icons": "^5.1",
    "@fortawesome/vue-fontawesome": "^0.0.22",
    "bootstrap": "^4.1.2",
    "chroma-js": "^1.3.7",
    "d3": "^5.5.0",
    "d3-tip": "^0.9.1",
    "granim": "^1.1.1",
    "lodash": "^4.17.10",
    "vue": "^2.5.16",
    "vue-lazyload": "^1.2.6",
    "vue-masonry-css": "^1.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.5",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.5",
    "@vue/cli-service": "^3.0.0-rc.5",
    "@vue/eslint-config-airbnb": "^3.0.0-rc.5",
    "async": "^2.6.1",
    "fs": "^0.0.1-security",
    "gh-pages": "^1.2.0",
    "gulp": "^4.0.0",
